<table>
	<thead>
		<th>会員ID</th>
		<th></th>
		<th>名前</th>

		<th>登録日</th>
	</thead>
	<tbody>
		<% users.each do |user| %>
			<tr>
				<td><%= user.id %></td>
				<td></td>
				<td><%= user.name %></td>
				<td><%= user.created_at.strftime('%Y/%m/%d') %></td>
				<td><%= link_to "削除", admin_user_path(user),method: :delete, class: "btn btn-danger" %></td>
			</tr>
		<% end %>
	</tbody>
</table>